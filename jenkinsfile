pipeline {
    agent any
    tools {
          maven 'Maven'
      }
     stages {
         stage('clone repository') {
            steps {
               checkout scm
               }
            }
          stage('Build') {
             steps {
                sh 'mvn clean package -DskipTests'             
             }          
          }
          stage('createAnInstance')
              {
          steps {
                          sh 'this i tmp step'
                    }
                    }
         }
     }
